package com.masai;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class A {
	@Autowired(required = false)
	private B b1;

	
	public void funA() {

	
	System.out.println("iside funA of A class");
	System.out.println(b1);
	b1.funB();
}
}
